def sort(nums):
//first sets of loop
    for i in range(len(nums)-1,0,-1):
        //going on next iteration 
        for j in range(i):
            if nums[j]>nums[j+1]:
                temp = nums[j]
                nums[j] = nums[j+1]
                nums[j+1] = temp

//numbers want to sort
nums = [5, 3, 8, 6, 7, 2]
sort(nums)

print(nums)

return ():
